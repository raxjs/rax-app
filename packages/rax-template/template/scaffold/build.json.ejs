<%_ const isSSR = projectFeatures.includes('ssr') -%>
<%_ const isServerless = projectFeatures.includes('serverless') -%>
{
  "plugins": [
    [
      "rax-plugin-app",
      {
        "targets": ["<%- projectTargets.join('", "') %>"]
      }
    ]<%_ if (isServerless) { -%>,<%_ } %>
    <%_ if (isServerless) { -%>
    [
      "rax-plugin-faas",
      {
        "aliyunConfig": {
          "id": "<%= projectAliyunId %>",
          "region": "<%= projectServerlessRegion %>"
        },
        "functionGroup": {
          "name": "raxFaaS",
          "root": "src/api",
          "functions": {
            "user": {
              "path": "user"
            }
          }
        }
      }
    ]<%_ } -%><%_ if (isSSR) { -%>,<%_ } %>
    <%_ if (isSSR) { -%>
    "rax-plugin-ssr"
    <%_ } -%>
  ]
}
