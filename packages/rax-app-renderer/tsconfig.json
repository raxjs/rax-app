{
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": "src",
    "outDir": "lib",
    "jsx": "react",
    "jsxFactory": "createElement",
    "module": "esNext",
    "target": "ES2016",
    "experimentalDecorators": true,
    "declaration": true,
    "sourceMap": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowJs": true
  },
}
