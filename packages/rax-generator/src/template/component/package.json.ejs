{
  "name": "<%= npmName %>",
  "author": "<%= projectAuthor %>",
  "version": "0.0.0",
  "description": "",
  "main": "lib/index.js",
  "module": "src/index.js",
  "miniappConfig": {
    "main": "lib/miniapp/index",
    "main:wechat": "lib/wechat-miniprogram/index"
  },
  "files": [
    "dist",
    "es",
    "lib",
    "src",
    "types"
  ],
  "keywords": [
    "Rax",
    "rax-component"
  ],
  "engines": {
    "npm": ">=3.0.0"
  },
  "peerDependencies": {
    "rax": "^1.1.0"
  },
<%_ if(componentType === 'ui') { -%>
  "scripts": {
    "start": "build-scripts start",
    "build": "build-scripts build",
    "lint": "eslint ./src/**/*.*",
    "lint-fix": "eslint --fix ./src/**/*.*",
    "release": "npm run lint"
  },
  "dependencies": {
    "@rax-ui/styles": "^1.0.0-beta.21",
    "rax-text": "^1.0.0",
    "rax-view": "^1.0.0"
  },
  "devDependencies": {
    <%_ if (languageType === 'ts') { -%>
    "typescript": "^3.7.5",
    "rax-types": "^1.0.0",
    <%_ } -%>
    "@alib/build-scripts": "^0.1.0",
    "@ice/spec": "^0.1.9",
    "@types/ali-app": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "build-plugin-rax-component": "^0.2.0",
    "build-plugin-multi-demo-portal": "^0.1.0",
    "commitlint": "^8.2.0",
    "driver-universal": "^3.1.0",
    "eslint": "^6.7.2",
    "eslint-config-rax": "^0.0.3",
    "eslint-plugin-import": "^2.19.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "rax": "^1.1.0",
    "rax-test-renderer": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS --verbose",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
<%_ } else { -%>
  "scripts": {
    "start": "build-scripts start",
    "build": "build-scripts build"
  },
  "dependencies": {
    "rax-text": "^1.0.0",
    "rax-view": "^1.0.0"
  },
  "devDependencies": {
    <%_ if (languageType === 'ts') { -%>
    "typescript": "^3.7.5",
    "rax-types": "^1.0.0",
    <%_ } -%>
    "@alib/build-scripts": "^0.1.0",
    "build-plugin-rax-component": "^0.2.0",
    "driver-universal": "^3.1.0",
    "rax": "^1.1.0",
    "rax-test-renderer": "^1.0.0"
  }
<%_ }  -%>
}
