<% Object.keys(usingPlugins).forEach(function(pluginTag){ %>
  <template name="<%= pluginTag %>">
    <<%=pluginTag%>
      <% usingPlugins[pluginTag].props.forEach(function(prop){%>
        <%= prop %>="{{r.<%= prop %>}}"
      <% });%>
      <% usingPlugins[pluginTag].events.forEach(function(event){%>
        <%= `bind${event}` %>="r.<%= event %>"
      <% });%>
    >
    </<%=pluginTag%>>
  </template>
<% }); %>
