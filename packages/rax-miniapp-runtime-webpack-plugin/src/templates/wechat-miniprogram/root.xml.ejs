<template name="video">
  <video
    style="{{r.style}}"
    class="{{r.class}}"
    hidden="{{r.hidden}}"
    id="{{r.id}}"
    data-private-node-id="{{r.nodeId}}"
    data-private-page-id="{{r.pageId}}"

    bindtap="onTap"
    bindtouchstart="onTouchStart"
    bindtouchmove="onTouchMove"
    bindtouchend="onTouchEnd"
    bindtouchcancel="onTouchCancel"

    bindplay="onVideoPlay"
    bindpause="onVideoPause"
    bindended="onVideoEnded"
    bindtimeupdate="onVideoTimeUpdate"
    bindfullscreenchange="onVideoFullScreenChange"
    bindwaiting="onVideoWaiting"
    binderror="onVideoError"
    bindprogress="onVideoProgress"

    src="{{r.src}}"
    duration="{{r.duration}}"
    controls="{{r.controls}}"
    danmu-list="{{r['danmu-list']}}"
    danmu-btn="{{r['danmu-btn']}}"
    enable-danmu="{{r['enable-danmu']}}"
    autoplay="{{r.autoplay}}"
    loop="{{r.loop}}"
    muted="{{r.muted}}"
    initial-time="{{r['initial-time']}}"
    direction="{{r.direction}}"
    show-progress="{{r['show-progress']}}"
    show-fullscreen-btn="{{r['show-fullscreen-btn']}}"
    show-play-btn="{{r['show-play-btn']}}"
    show-center-play-btn="{{r['show-center-play-btn']}}"
    enable-progress-gesture="{{r['enable-progress-gesture']}}"
    object-fit="{{r['object-fit']}}"
    poster="{{r.poster}}"
    show-mute-btn="{{r['show-mute-btn']}}"
    title="{{r.title}}"
    play-btn-position="{{r['play-btn-position']}}"
    enable-play-gesture="{{r['enable-play-gesture']}}"
    auto-pause-if-navigate="{{r['auto-pause-if-navigate']}}"
    auto-pause-if-open-native="{{r['auto-pause-if-open-native']}}"
    vslide-gesture="{{r['vslide-gesture']}}"
    vslide-gesture-in-fullscreen="{{r['vslide-gesture-in-fullscreen']}}"
  >
  </video>
</template>

<template name="camera">
  <camera
    style="{{r.style}}"
    hidden="{{r.hidden}}"
    id="{{r.id}}"
    data-private-node-id="{{r.nodeId}}"
    data-private-page-id="{{r.pageId}}"

    mode="{{r.mode || 'normal'}}"
    device-position="{{r['device-position'] || 'back'}}"

    bindstop="onCameraStop"
    binderror="onCameraError"
    bindinitdone="onCameraInitDone"
    bindscancode="onCameraScanCode"
 />
</template>

<template name="map">
  <map
    style="{{r.style}}"
    class="{{r.class}}"
    hidden="{{r.hidden}}"
    id="{{r.id}}"
    data-private-node-id="{{r.nodeId}}"
    data-private-page-id="{{r.pageId}}"

    bindtap="onTap"
    bindtouchstart="onTouchStart"
    bindtouchmove="onTouchMove"
    bindtouchend="onTouchEnd"
    bindtouchcancel="onTouchCancel"

    bindmarkertap="onMapMarkerTap"
    bindcontroltap="onMapControlTap"
    bindcallouttap="onMapCalloutTap"
    bindupdated="onMapUpdated"
    bindregibindchange="onMapRegionChange"

    longitude="{{r.longitude !== undefined ? r.longitude : 39.92}}"
    latitude="{{r.latitude !== undefined ? r.latitude : 116.46}}"
    scale="{{r.scale !== undefined ? r.scale : 16}}"
    markers="{{r.markers !== undefined ? r.markers : []}}"
    polyline="{{r.polyline !== undefined ? r.polyline : []}}"
    circles="{{r.circles !== undefined ? r.circles : []}}"
    controls="{{r.controls !== undefined ? r.controls : []}}"
    include-points="{{r['include-points'] !== undefined ? r['include-points'] : []}}"
    show-location="{{r['show-location']}}"
    polygons="{{r.polygons !== undefined ? r.polygons : []}}"
    subkey="{{r.subkey || ''}}"
    layer-style="{{r['layer-style'] !== undefined ? r['layer-style'] : 1}}"
    rotate="{{r.rotate || 0}}"
    skew="{{r.skew || 0}}"
    enable-3D="{{r['enable-3D']}}"
    show-compass="{{r['show-compass']}}"
    enable-overlooking="{{r['enable-overlooking']}}"
    enable-zoom="{{r['enable-zoom']}}"
    enable-scroll="{{r['enable-scroll']}}"
    enable-rotate="{{r['enable-rotate']}}"
    enable-satellite="{{r['enable-satellite']}}"
    enable-traffic="{{r['enable-traffic']}}"
  >
    <template is="children" data="{{r: r.children}}" />
  </map>
</template>

<template name="canvas">
  <canvas
    style="{{r.style}}"
    class="{{r.class}}"
    hidden="{{r.hidden}}"
    id="{{r.id}}"
    data-private-node-id="{{r.nodeId}}"
    data-private-page-id="{{r.pageId}}"

    bindtap="onTap"
    bindtouchstart="onTouchStart"
    bindtouchmove="onTouchMove"
    bindtouchend="onTouchEnd"
    bindtouchcancel="onTouchCancel"

    bindlongpress="onCanvasLongTap"
    binderror="onCanvasError"

    type="{{r.type}}"
    canvas-id="{{r['canvas-id']}}"
    disable-scroll="{{r['disable-scroll']}}"
  >
  </canvas>
</template>

<template name="web-view">
  <web-view
    style="{{r.style}}"
    class="{{r.class}}"
    hidden="{{r.hidden}}"
    id="{{r.id}}"
    data-private-node-id="{{r.nodeId}}"
    data-private-page-id="{{r.pageId}}"

    bindtap="onTap"
    bindtouchstart="onTouchStart"
    bindtouchmove="onTouchMove"
    bindtouchend="onTouchEnd"
    bindtouchcancel="onTouchCancel"

    bindmessage="onWebviewMessage"
    bindload="onWebviewLoad"
    binderror="onWebviewError"

    src="{{r.src}}"
  >
  </web-view>
</template>
